<div class="modal fade" data-backdrop="static" id="modifyStatusDialog" tabindex="-1" role="dialog" aria-labelledby="modifyStatusLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="modifyStatusLabel">Modify Workflow Instance Status</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">                                            
                <form>    
                    <div style="text-align: left">
                        <div id='modifyStatus_config_alert' class="alert alert-danger"></div>                                                                              
                        Modify status of <b><span id="wfi_id"></span></b> to<br><br>
                        <div class="justify-content-start">
                            <div class="form-group">                                                          
                                <div class="form-check">
                                    <input class="form-check-input" type="radio" name="status_name" id="failed_option" value="FAILED" checked>
                                    <label class="form-check-label" for="failed_option">FAILED</label>
                                </div>
                                <div class="form-check">
                                    <input class="form-check-input" type="radio" name="status_name" id="comp_option" value="COMPLETE">
                                    <label class="form-check-label" for="comp_option">COMPLETE</label>
                                </div>
                                <div class="form-check">
                                    <input class="form-check-input" type="radio" name="status_name" id="compen_option" value="COMPLETE-PENDINGCLEANUP">
                                    <label class="form-check-label" for="compen_option">COMPLETE-PENDINGCLEANUP</label>
                                </div>
                            </div>                                                   
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">                                            
                <button type="button" class="btn btn-secondary" data-dismiss="modal" id="cancel_btn">Cancel</button>
                <button type="button" id="modifyButton" class="btn btn-success">Modify</button>                                                                                
            </div>
        </div>
    </div>
</div>
<div class="modal fade" data-backdrop="static" id="viewLogDialog" tabindex="-1" role="dialog" aria-labelledby="viewLogLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="viewLogLabel">Error Logs</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">          
                <div id='log_config_alert' class="alert alert-warning"></div>                                    
                <div id="err_log_display" style="text-align: left">                    
                    <input type="text" class="form-control" id="log_srch_box" placeholder="Search logs"/>                                                                                                                  
                     <div class="table-responsive">
                        <table class="table table-hover table-sm" id="log_table">                                                                                                              
                        </table>
                    </div>                    
                </div>
            </div>
            <div class="modal-footer">                                            
                <button type="button" class="btn btn-secondary" data-dismiss="modal" id="close_btn">Close</button>                
            </div>
        </div>
    </div>            
</div>
<div class="modal fade" data-backdrop="static" id="preCompileDialog" tabindex="-1" role="dialog" aria-labelledby="precompileLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
